{
    "name": "CAP Development Container",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "VARIANT": "24"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "mechatroner.rainbow-csv",
                "dbaeumer.vscode-eslint",
                "sapse.vscode-cds",
                "sapse.vsc-extension-odata-csdl-modeler",
                "sapse.vscode-wing-cds-editor-vsc",
                "saposs.xml-toolkit"
            ]
        }
    },
    "forwardPorts": [
        4004,
        4005,
        4006,
        5005,
        9229
    ],
    "remoteUser": "node"
}
